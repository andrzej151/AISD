package Haszowanie;

public class TabHasz implements Hasz{

	private Object [] tab;
	
	public  TabHasz() {
		// TODO Auto-generated constructor stub
		tab = new Object[13];
	}
	
	@Override
	public Object get(int k) {
		// TODO Auto-generated method stub
		int kl=k%tab.length;
		int i =1;
		
		while(tab[kl]!=null)
		{
			kl=(kl+i*i)%tab.length;
			i++;
		}
		return tab[kl];
	}

	@Override
	public void put(int k, Object v) {
		// TODO Auto-generated method stub
		int kl=k%tab.length;
		int i =1;
		while(tab[kl]!=null)
		{
			kl=(kl+i*i)%tab.length;
			i++;
		}
		tab[kl]=v;
		
	}

	@Override
	public Boolean containsKey(int k) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public int size() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public Boolean isEmpty() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void resize() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void dump() {
		// TODO Auto-generated method stub
		for(int i = 0;i<tab.length;i++)
		{
			if(tab[i]==null)
			{
				System.out.print("* ");
			}
			else
			{
				System.out.print(i+" ");
			}
		}
		System.out.println("");
	}
	
	

}
